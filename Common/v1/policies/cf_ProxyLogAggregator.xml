<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="cf_ProxyLogAggregator">
    <DisplayName>cf_ProxyLogAggregator</DisplayName>
    <Properties/>
    <!-- Proxy Log Aggregator -->
    <AssignVariable>
        <Name>log.startTimeStamp</Name>
        <Ref>system.timestamp</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyVerb</Name>
        <Ref>request.verb</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyScheme</Name>
        <Ref>client.scheme</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyHost</Name>
        <Ref>request.header.host</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyURI</Name>
        <Ref>request.uri</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyRequest</Name>
        <Ref>request.content</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyStartTimeStamp</Name>
        <Ref>client.received.start.timestamp</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyEndTimeStamp</Name>
        <Ref>client.received.end.timestamp</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.clientIP</Name>
        <Ref>client.ip</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.clientHost</Name>
        <Ref>client.host</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.clientPort</Name>
        <Ref>client.port</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.uuid</Name>
        <Ref>system.uuid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyName</Name>
        <Ref>proxy.name</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.proxyRevision</Name>
        <Ref>apiproxy.revision</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.environmentName</Name>
        <Ref>environment.name</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.organizationName</Name>
        <Ref>organization.name</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>log.isError</Name>
        <Ref>is.error</Ref>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
